class Game {
  field Snake snake;
  field int numCoins;

  constructor Game new(int argSnakeLength, int argNumCoins) {
    let snake = Snake.new(argSnakeLength);
    let numCoins = argNumCoins;
    return this;
  }

  method void play() {
    do snake.draw(0, 0);
    return;
  }

  method void dispose() {
    return;
  }
}
