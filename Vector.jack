class Vector {
  field int xComponent;
  field int yComponent;

  constructor Vector new(int argXComponent, int argYComponent) {
    let xComponent = argXComponent;
    let yComponent = argYComponent;
    return this;
  }

  method int xComponent() {
    return xComponent;
  }

  method int yComponent() {
    return yComponent;
  }
}
